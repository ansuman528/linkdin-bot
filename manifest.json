{
    "manifest_version": 3,
    "name": "Linkdin Bot",
    "description": "Extract Contact information from linkdin Ids ",
    "version": "1.0",
    "icons":{
      "16":"heart.png"
    },
    "permissions": ["tabs","activeTab","scripting","storage"],
    "host_permissions": [
      "http://*/*",
      "https://*/*"
    ],
    "action":{
      "default_popup":"popup.html"
    }
}